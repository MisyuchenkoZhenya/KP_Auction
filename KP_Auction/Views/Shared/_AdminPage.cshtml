<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Main page</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <div class="pagination btn-group">
        @Html.ActionLink("Auctions", "GetAll", "Auction", null, new { @class = "btn btn-primary" })
        @Html.ActionLink("Deals", "GetAll", "Deal", null, new { @class = "btn btn-primary" })
        @Html.ActionLink("DealStates", "GetAll", "DealState", null, new { @class = "btn btn-primary" })
        @Html.ActionLink("ItemCategories", "GetAll", "ItemCategory", null, new { @class = "btn btn-primary" })
        @Html.ActionLink("Items", "GetAll", "Item", null, new { @class = "btn btn-primary" })
        @Html.ActionLink("Participants", "GetAll", "Participant", null, new { @class = "btn btn-primary" })
        @Html.ActionLink("TradingProgresses", "GetAll", "TradingProgress", null, new { @class = "btn btn-primary" })
    </div>
    @RenderBody()

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
